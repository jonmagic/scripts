{
  "name": "jonmagic-scripts-raycast",
  "title": "Jonmagic Scripts",
  "description": "Create Daily Project notes in my Brain repo",
  "icon": "extension-icon.png",
  "author": "jonmagic",
  "license": "MIT",
  "categories": ["Productivity"],
  "platforms": ["macOS"],
  "commands": [
    {
      "name": "create-daily-project-note",
      "title": "Create Daily Project Note",
      "description": "Creates a numbered note in Daily Projects/YYYY-MM-DD",
      "mode": "view"
    }
  ],
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ray develop",
    "build": "ray build",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "publish": "npx @raycast/api@latest publish",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@jonmagic/scripts-core": "workspace:*",
    "@raycast/api": "^1.82.1",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "typescript": "^5.8.2"
  }
}
